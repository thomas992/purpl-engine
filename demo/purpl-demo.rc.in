        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "@PURPL_APP_ORGANIZATION@\0"
            VALUE "FileDescription", "@PURPL_APP_DISPLAY_NAME@\0"
            VALUE "FileVersion", "@version_string@\0"
            VALUE "InternalName", "@target@\0"
            VALUE "LegalCopyright", "Copyright (C) 2022 @PURPL_APP_ORGANIZATION@\0"
            VALUE "OriginalFilename", "@target@.exe\0"
            VALUE "ProductName", "@PURPL_APP_DISPLAY_NAME@\0"
            VALUE "ProductVersion", "@version@\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
